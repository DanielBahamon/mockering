  <div class="_fullscreen_absolute">
    <div class="d-table fullsize">
      <div class="d-table-cell v-middle">
        <div class="row">
          <div class="col-12 text-center">
            <h3>Share your content</h3>
            <br>
          </div>
        </div>
        <div class="row">
          <div class="col-12 col-sm-6 col-md-4 offset-md-2 col-lg-3 offset-lg-3">
            <button class="_btn_form_kind" id="collapse_add_mocket" type="button" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
              <div class="row">
                <div class="col-12 text-center">
                  <img src="//d8mn60sm70fqt.cloudfront.net/assets/svg/videocamera.svg">
                </div>
                <div class="col-12 text-center">
                  <b>New Mock</b><br/>
                  <small class="text-muted">MP4</small>
                </div>
              </div>
            </button>
          </div>
          <div class="col-12 col-sm-6 col-md-4  col-lg-3">
            <button class="_btn_form_kind" id="collapse_add_mock" type="button" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
              <div class="row">
                <div class="col-12 text-center">
                  <img src="//d8mn60sm70fqt.cloudfront.net/assets/svg/picture.svg">
                </div>
                <div class="col-12 text-center">
                  <b>New Mocket</b><br/>
                  <small class="text-muted">JPG/PNG/GIF</small>
                </div>
              </div>
            </button>
          </div>
        </div>
      </div>
    </div>
  </div>
