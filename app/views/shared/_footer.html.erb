<footer class="_footer _1">
	<div class="row m-0">
		<div class="col-md-4 _1">
      <%= link_to root_path, data: { no_turbolink: true }, class: "_link_logo _brand" do %>
        <%= image_tag '//d8mn60sm70fqt.cloudfront.net/assets/brand/v2/mockering_1.svg', class: "_logonav _2 d-none-moon", alt: 'Mockering'%>
