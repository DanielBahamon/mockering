          <% end %>
          <%= link_to howmockeringworks_path, class: "_link" do %>
          	Políticas
          <% end %>
          <%= link_to privacy_path, class: "_link" do %>
          	Privacidad
          <% end %>
          <%= link_to terms_path, class: "_link" do %>
          	Terminos
          <% end %>
				</div>
			</div>
		</div>
	</div>
</footer>
