			<div class="row m-0">
				<div class="col-sm-4 p-0">
					<b>Acceso</b>
          <%= link_to new_mocker_session_path, class: "_link" do %>
          	Iniciar sesión
          <% end %>
          <%= link_to new_mocker_registration_path, class: "_link" do %>
          	Primera vez
          <% end %>
          <%= link_to new_mocker_password_path, class: "_link" do %>
